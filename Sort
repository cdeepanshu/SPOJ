import java.util.*;
class sort
{
public static void main(String args[])
{
int a[]=new int[10];
int temp,i;
Scanner s=new Scanner(System.in);
System.out.println("Enter the array");
for(i=0;i<10;i++)
a[i]=s.nextInt();

for(i=0;i<10;i++)
{
for(int j=i+1;j<10;j++)
{
if(a[i]>a[j])
{
temp=a[i];
a[i]=a[j];
a[j]=temp;
}
}
}
System.out.println("after the sorting");
for(i=0;i<10;i++)
System.out.println(a[i]);
}
}

